<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:background="@color/f0_gray_line"
	android:layout_width="match_parent"
	android:orientation="vertical"
	android:layout_height="match_parent"
	android:fitsSystemWindows="true">
	<include layout="@layout/include_title"/>
	<RelativeLayout
		android:id="@+id/rl_wechat_pay"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="@drawable/selector_item_bg">
		<ImageView
			android:id="@+id/iv_wechat"
			android:layout_width="@dimen/dp_25"
			android:layout_height="@dimen/dp_25"
			android:layout_centerVertical="true"
			android:layout_alignParentLeft="true"
			android:src="@drawable/account_wechat"
			android:layout_marginLeft="@dimen/dp_15"
			android:layout_marginRight="@dimen/dp_13"
			/>
		<TextView
			android:id="@+id/tv_wechat"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="提现微信"
			android:gravity="center_vertical"
			android:layout_toRightOf="@+id/iv_wechat"
			android:layout_marginTop="@dimen/dp_15"
			android:textColor="@color/all_black"
			android:textSize="@dimen/sp_15"/>
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textColor="@color/gray_99"
			android:textSize="@dimen/sp_12"
			android:layout_below="@id/tv_wechat"
			android:text="推荐安装微信的用户使用"
			android:layout_toRightOf="@+id/iv_wechat"
			android:layout_marginTop="@dimen/dp_7"
			android:layout_marginBottom="@dimen/dp_15"/>
		<ToggleButton
			android:id="@+id/tBtnSecretWechat"
			android:layout_width="@dimen/dp_20"
			android:layout_height="@dimen/dp_20"
			android:layout_alignParentRight="true"
			android:layout_centerVertical="true"
			android:background="@null"
			android:button="@drawable/wallet_switch_btn"
			android:checked="false"
			android:layout_marginRight="@dimen/dp_15"
			android:textOff=""
			android:textOn=""/>
	</RelativeLayout>
	<RelativeLayout
		android:id="@+id/rl_ali_pay"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="1px"
		android:background="@drawable/selector_item_bg">
		<ImageView
			android:id="@+id/iv_alipay"
			android:layout_width="@dimen/dp_25"
			android:layout_height="@dimen/dp_25"
			android:layout_centerVertical="true"
			android:layout_alignParentLeft="true"
			android:src="@drawable/ic_paytype_ali"
			android:layout_marginLeft="@dimen/dp_15"
			android:layout_marginRight="@dimen/dp_13"
			/>
		<TextView
			android:id="@+id/tv_alipay"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="提现支付宝"
			android:gravity="center_vertical"
			android:layout_toRightOf="@+id/iv_alipay"
			android:layout_marginTop="@dimen/dp_15"
			android:textColor="@color/all_black"
			android:textSize="@dimen/sp_15"/>
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textColor="@color/gray_99"
			android:textSize="@dimen/sp_12"
			android:layout_below="@id/tv_alipay"
			android:text="推荐有支付宝账户的用户使用"
			android:layout_toRightOf="@+id/iv_alipay"
			android:layout_marginTop="@dimen/dp_7"
			android:layout_marginBottom="@dimen/dp_15"/>
		<ToggleButton
			android:id="@+id/tBtnSecretAlipay"
			android:layout_width="@dimen/dp_20"
			android:layout_height="@dimen/dp_20"
			android:layout_alignParentRight="true"
			android:layout_centerVertical="true"
			android:background="@null"
			android:button="@drawable/wallet_switch_btn"
			android:checked="true"
			android:layout_marginRight="@dimen/dp_15"
			android:textOff=""
			android:textOn=""/>
	</RelativeLayout>
	<TextView
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="@dimen/dp_4"
		android:background="@color/white"
		android:padding="@dimen/dp_15"
		android:textColor="@color/black_66"
		android:text="提现金额"
		android:gravity="center_vertical"
		android:textSize="@dimen/dp_15"/>
	<RelativeLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:paddingBottom="@dimen/dp_15"
		android:background="@color/white">
		<TextView
			android:id="@+id/tv_unit"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="@dimen/dp_15"
			android:textSize="@dimen/sp_25"
			android:textColor="@color/all_black"
			android:text="￥"/>
		<EditText
			android:id="@+id/et_money"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_toRightOf="@+id/tv_unit"
			android:textSize="@dimen/sp_36"
			android:layout_alignBaseline="@id/tv_unit"
			android:maxLength="9"
			android:gravity="left|center_vertical"
			android:layout_marginRight="@dimen/dp_15"
			android:layout_marginLeft="@dimen/dp_12"
			android:inputType="numberSigned"
			android:digits="0123456789."
			android:textColor="@color/all_black"
			android:textColorHint="@color/gray_99"
			android:visibility="gone"
			android:background="@null"/>
		<TextView
			android:id="@+id/tv_money_hint"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_toRightOf="@+id/tv_unit"
			android:textSize="@dimen/sp_15"
			android:layout_alignBaseline="@id/tv_unit"
			android:maxLength="9"
			android:gravity="left|center_vertical"
			android:layout_marginRight="@dimen/dp_15"
			android:layout_marginLeft="@dimen/dp_12"
			android:inputType="numberSigned"
			android:digits="0123456789."
			android:textColor="@color/black_66"
			android:textColorHint="@color/gray_99"
			android:background="@null"
			android:hint="提现金额须>=1元(单位：元)"/>
		<View android:id="@+id/line_money" style="@style/line_c8Style" android:layout_below="@id/tv_unit" android:layout_margin="@dimen/dp_15"/>
		<TextView
			android:id="@+id/tv_money"
			android:textColor="@color/black_66"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:gravity="center_vertical"
			android:layout_marginLeft="@dimen/dp_15"
			android:layout_below="@+id/line_money"
			android:layout_alignParentLeft="true"
			android:textSize="@dimen/sp_12"/>
		<TextView
			android:id="@+id/tv_all_money"
			android:textColor="@color/blue_ff"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentRight="true"
			android:layout_below="@id/line_money"
			android:gravity="center_vertical"
			android:layout_marginRight="@dimen/dp_15"
			android:text="全部提现"
			android:textSize="@dimen/sp_12"/>
	</RelativeLayout>
	<RelativeLayout
		android:id="@+id/rl_alipay"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="@color/white"
		android:layout_marginTop="@dimen/dp_4"
		android:visibility="visible">
		<TextView
			android:id="@+id/tv_alipay_txt"
			android:textColor="@color/all_black"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerVertical="true"
			android:layout_alignParentLeft="true"
			android:layout_margin="@dimen/dp_15"
			android:text="支付宝账号:"
			android:gravity="center_vertical"
			android:textSize="@dimen/sp_15"/>
		<EditText
			android:id="@+id/et_alipay"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_toRightOf="@id/tv_alipay_txt"
			android:layout_centerVertical="true"
			android:textSize="@dimen/sp_15"
			android:gravity="left|center_vertical"
			android:paddingRight="@dimen/dp_15"
			android:inputType="text"
			android:textColor="@color/black_66"
			android:textColorHint="@color/gray_99"
			android:background="@null"
			android:hint="输入支付宝账号"/>
	</RelativeLayout>
	<TextView
		android:layout_width="wrap_content"
		android:layout_height="0dp"
		android:layout_weight="1"/>
		<TextView
			android:id="@+id/txtWithdraw"
			android:layout_width="match_parent"
			android:layout_height="@dimen/dp_44"
			android:layout_margin="@dimen/dp_15"
			android:background="@drawable/button_login_selector"
			android:text="提现"
			android:gravity="center"
			android:textSize="@dimen/sp_16"
			android:textColor="@color/white" />
</LinearLayout>
