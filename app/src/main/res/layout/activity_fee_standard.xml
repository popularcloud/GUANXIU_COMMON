<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_fee_standard"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.lwc.common.module.order.ui.activity.FeeStandardActivity"
    android:orientation="vertical"
    android:fitsSystemWindows="true"
    android:background="@color/f0_gray_line">
    <include layout="@layout/include_title" android:id="@+id/include_title"/>
    <ExpandableListView
        android:id="@+id/expand_list"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:groupIndicator="@null"
        android:divider="@null"
        android:childDivider="@color/f0_gray_line"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp_15"
        android:orientation="vertical">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/red_money"
            android:textSize="@dimen/sp_12"
            android:text="备注"/>
        <TextView
            android:id="@+id/tv_remark"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_11"
            android:lineSpacingMultiplier="1.5"
            android:textSize="@dimen/sp_12"
            android:textColor="@color/black_66"/>
    </LinearLayout>
</LinearLayout>
